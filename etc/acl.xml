<?xml version="1.0"?>
<!--
/* Wise fancy box designer */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::brand_elements" title="Wise Fancy">
                    <resource id="Wise_Fancy::brand" title="Brands" sortOrder="10">
                        <resource id="Wise_Fancy::brand_edit" title="Edit Brand" sortOrder="10" />
                        <resource id="Wise_Fancy::brand_save" title="Save Brand" sortOrder="20" />
                        <resource id="Wise_Fancy::brand_delete" title="Delete Brand" sortOrder="30" />
                    </resource>
                    <resource id="Wise_Fancy::group" title="Groups" sortOrder="30">
                        <resource id="Wise_Fancy::group_edit" title="Edit Group" sortOrder="10" />
                        <resource id="Wise_Fancy::group_save" title="Save Group" sortOrder="20" />
                        <resource id="Wise_Fancy::group_delete" title="Delete Group" sortOrder="30" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Wise_Fancy::config_brand" title="Brand" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>